{
  "name": "api-supercharger",
  "private": true,
  "version": "0.0.1",
  "description": "REST API Supercharger",
  "license": "MIT",
  "author": {
    "name": "Ognjen Bostjancic",
    "url": "https://ognjenbostjancic.com",
    "email": "ognjen.bostjancic@gmail.com"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release:patch": "yarn version --patch --no-git-tag-version",
    "release:minor": "yarn version --minor --no-git-tag-version",
    "release:major": "yarn version --major --no-git-tag-version"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^7.6.13",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/swagger": "^4.7.13",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/passport-local": "^1.0.33",
    "auth0": "^2.33.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "dotenv": "^16.0.0",
    "jwks-rsa": "^2.0.5",
    "nest-access-control": "^2.0.2",
    "nestjs-typeorm-paginate": "^3.1.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "root-require": "^0.3.1",
    "rxjs": "^6.5.5",
    "shell": "^0.9.0",
    "shelljs": "^0.8.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.13",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
